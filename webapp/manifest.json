{
  "_version": "1.28.0",
  "sap.app": {
    "id": "br.com.gestao.fioriappreport398",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.3",
      "toolsId": "faa0b5ac-0236-45eb-b573-82a7a4798b8d"
    },
    "dataSources": {
      "ZSB_PRODUCTION_398_VAN": {
        "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='ZSB_PRODUCTION_398_VAN',Version='0001')/$value/",
        "type": "ODataAnnotation",
        "settings": {
          "localUri": "localService/mainService/ZSB_PRODUCTION_398_VAN.xml"
        }
      },
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZSB_PRODUCTION_398/",
        "type": "OData",
        "settings": {
          "annotations": [
            "ZSB_PRODUCTION_398_VAN"
          ],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "fullWidth": true,
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.84.44",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.uxap": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "br.com.gestao.fioriappreport398.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "br.com.gestao.fioriappreport398.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false,
        "bypassed": {
          "target": "notFound"
        }
      },
      "routes": [
        {
          "name": "RouteApp",
          "pattern": "RouteApp",
          "target": [
            "TargetApp"
          ]
        },
        {
          "name": "Lista",
          "pattern": "",
          "target": [
            "lista"
          ]
        },
        {
          "name": "Detalhes",
          "pattern": "detalhes/{Productid}",
          "target": [
            "detalhes"
          ]
        }
      ],
      "targets": {
        "TargetApp": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "App",
          "viewName": "App"
        },
        "lista": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Lista",
          "viewName": "Lista",
          "controlId": "app"
        },
        "detalhes": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Detalhes",
          "viewName": "Detalhes",
          "controlId": "app"
        },
        "notFound": {
          "viewId": "notFound",
          "viewName": "notFound",
          "transition": "slide"
        },
        "objNotFound": {
          "viewId": "ObjectNotFound",
          "viewName": "ObjectNotFound",
          "transition": "slide"
        }
      }
    },
    "rootView": {
      "viewName": "br.com.gestao.fioriappreport398.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    }
  }
}